pipeline {
  agent any
  stages {
    stage('version') {
      steps {
          echo 'Running version stage'
          bat 'C:\Program Files\python.exe --version'         
      }
    }
    stage('hello') {
      steps {
          echo 'Running hello stage'
          bat 'C:\Program Files\python.exe hello.py'  
      }
    }
  }
}
